{
    "name": "NameM6",
    "description": "First test build",
    "version": "0.2",
    "author": "M6 & Faav",
    "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
    },
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["https://*.namemc.com/search*"],
            "js": ["nameSearch.js"]
        }
    ],
    "permissions": ["activeTab"],
    "web_accessible_resources": [{
        "resources": ["js/*"],
        "matches": ["https://namemc.com/*"]
    }]
}
